{
  "name": "eslint-import-resolver-babel-module",
  "version": "5.0.0-beta.1",
  "main": "lib/index.js",
  "description": "babel-plugin-module-resolver resolver for eslint-plugin-import",
  "repository": {
    "type": "git",
    "url": "https://github.com/tleunen/eslint-import-resolver-babel-module.git"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "files": [
    "lib"
  ],
  "author": "Tommy Leunen <tommy.leunen@gmail.com> (https://tommyleunen.com)",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslint-plugin-import",
    "eslint-import-resolver",
    "babel",
    "babel-plugin",
    "module",
    "resolver",
    "alias",
    "rewrite",
    "resolve",
    "rename",
    "mapping",
    "require",
    "import"
  ],
  "dependencies": {
    "pkg-up": "^2.0.0",
    "resolve": "^1.8.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-rc.1",
    "@babel/core": "^7.0.0-rc.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-rc.1",
    "@babel/preset-env": "^7.0.0-rc.1",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^23.4.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "jest": "^23.5.0",
    "lodash": "^4.17.10",
    "standard-version": "^4.4.0"
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0-0",
    "babel-plugin-module-resolver": "^3.0.0"
  },
  "scripts": {
    "lint": "eslint src test",
    "compile": "babel src --out-dir lib",
    "pretest": "npm run lint",
    "test": "npm run test:suite --coverage",
    "test:suite": "jest",
    "test:watch": "jest --watch",
    "prepare": "npm run compile",
    "release": "standard-version"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "/test/.*\\.test\\.js$",
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  },
  "greenkeeper": {
    "ignore": [
      "babel-jest",
      "eslint",
      "eslint-plugin-import"
    ]
  }
}
